      <cit:party>
        <cit:CI_Individual>
          <cit:name>
            <gco:CharacterString>Robin Hood</gco:CharacterString>
          </cit:name>
          <cit:contactInfo xlink:href="#ip-protocol"/>
        </cit:CI_Individual>
      </cit:party>
    </cit:CI_Responsibility>
  </cit:citedResponsibleParty>
